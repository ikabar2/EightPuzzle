move([B1,B2,B3,W1,0,W2,W3],[B1,B2,B3,W1,W2,0,W3]).
move([B1,B2,B3,W1,0,W2,W3],[B1,B2,B3,W1,W3,W2,0]).

move([B1,B2,B3,W1,W2,0,W3],[B1,B2,B3,W1,W2,W3,0]).

move([B1,B2,0,B3,W1,W2,W3],[B1,B2,B3,0,W1,W2,W3]).
move([B1,B2,0,B3,W1,W2,W3],[B1,B2,W1,B3,0,W2,W3]).
